[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = A command line tool and library for transferring data with URL syntax, supporting HTTP, HTTPS, FTP, FTPS, GOPHER, TFTP, SCP, SFTP, SMB, TELNET, DICT, LDAP, LDAPS, FILE, IMAP, SMTP, POP3, RTSP and RTMP. libcurl offers a myriad of powerful features
[access "refs/*"]
	owner = group user/yiliayang-ytt
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/yiliayang-ytt
	label-Verified = -1..+1 group user/yiliayang-ytt
	push = group user/yiliayang-ytt
	read = group user/yiliayang-ytt
	rebase = group user/yiliayang-ytt
	removeReviewer = group user/yiliayang-ytt
	submit = group user/yiliayang-ytt
[access "refs/heads/*"]
	create = group user/yiliayang-ytt
	push = +force group user/yiliayang-ytt
	pushMerge = group user/yiliayang-ytt
	read = group user/yiliayang-ytt
[access "refs/tags/*"]
	push = group user/yiliayang-ytt
	push = +force group user/yiliayang-ytt
